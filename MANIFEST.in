# AWS CostLens - MANIFEST.in
# Include files in source distribution

include LICENSE
include README.md
include requirements.txt
include pyproject.toml
include config.example.yaml

# Exclude development files
exclude .gitignore
exclude .env
exclude .env.example
exclude mypy.ini
exclude Dockerfile
exclude docker-compose.yml

# Exclude output/test files
prune output
prune reports
prune __pycache__
prune *.egg-info
